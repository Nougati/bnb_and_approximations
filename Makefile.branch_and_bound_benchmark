CC=gcc
CC_FLAGS = -g -Wall -Wextra -fsanitize=address -fsanitize=undefined
CC_LIBRARIES=-lm

branch_and_bound_benchmark : branch_and_bound.o branch_and_bound_benchmark.o fptas.o pisinger_reader.o
	gcc branch_and_bound.o branch_and_bound_benchmark.o fptas.o pisinger_reader.o -o branch_and_bound_benchmark -g -lm 

branch_and_bound.o : branch_and_bound.c
	gcc -c branch_and_bound.c -g -lm

branch_and_bound_benchmark.o : branch_and_bound_benchmark.c
	gcc -c branch_and_bound_benchmark.c -g -lm

fptas.o : fptas.c
	gcc -c fptas.c -g -lm

pisinger_reader.o : pisinger_reader.c
	gcc -c pisinger_reader.c -g -lm

clean : 
	rm branch_and_bound.o branch_and_bound_benchmark.o fptas.o pisinger_reader.o branch_and_bound_benchmark
